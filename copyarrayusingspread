    <script>
            function arrayCopy(){
                let arr = ["1","2","3"];
                let arr2 = [...arr];

                //making changes in arr2
                arr2.push("4");
                arr2[0] = "99";

                console.log(arr);
                console.log(arr2);
            }
    </script>
    

    <button onclick="tryThis()";>try this</button>
    <script>
        'use strict';
        function tryThis() {
         
          let Name = 'Ishu';
          let  birth = new Date();
          let Alive = true;
          let Age = 23;
        
        console.log('Name : ' +Name.constructor.toString());
        console.log('birth : ' +birth.constructor.toString());
        console.log('Alive : ' +Alive.constructor.toString());
        console.log('Age : ' +Age.constructor.toString());
        }
    </script>
    

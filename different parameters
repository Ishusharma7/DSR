    <script>
        // default parameter
        function hell(name = 'Ishu'){
            console.log('hello ' +name);
        }s
            hell();
            hell('zoro');
        

        // //rest parameter
        let lora = function gret(...names){
                names.forEach(name => console.log('hi ' +name));

        }
        lora('ishu','arvind','nikhil');

        // //spread
        function display(char1, char2, char3){
            console.log(char1, char2, char3);
        }
        let numbers = '1233456';
        display(...numbers);
    </script>
    

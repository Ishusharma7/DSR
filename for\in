    <button onclick="details();">for\in</button>

    <script>
        function details(){
            let detail = {
            Name : "Ishu Sharma",
            Age : 23,
            Gender : "Male",
            behaviour : "Abnormal",
          };
        for (const Key in detail) {
            console.log(detail[Key] );
       }
    }
    </script>
    

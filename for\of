    <script>

        let detail = [   // [] are used to make collection of objects at once
        {   
            name : 'Ishu Sharma',
            age : 23,
            gender : 'Male' ,
        },
        {
            name : "John",
            age : 52,
            gender : 'Male',

        },
        {
            name : 'Natsu Dragneel',
            age : 4025,
            gender : 'male',
        },
                    //strings are iterable
                    

    ];
     function details(){
        for (const res of detail){
            console.log(JSON.stringify(res));
        }
     }
</script>

    <script>
      function carss(){
        let product = {
        Name : 'car',
        listPrice : 12000000,
        standardPrice : 100000,

            grossProfit : function hero () {
                return (this.listPrice - this.standardPrice)
                .toLocaleString('en-US' , {
                    style: 'currency', currency: 'USD'
                });
            }
      };
      console.log(product.grossProfit());
    }

    </script>
